{
  "name": "kubricate",
  "version": "0.5.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "kubricate": "dist/cli.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "mono dev",
    "start": "mono start",
    "build": "npm run build:cli && npm run build:library",
    "build:cli": "mono build:cli",
    "build:library": "mono build",
    "test": "mono test",
    "test:watch": "mono test:watch",
    "lint:check": "mono lint:check",
    "lint:fix": "mono lint:fix",
    "check-types": "mono check-types"
  },
  "devDependencies": {
    "@kubricate/config-eslint": "workspace:*",
    "@kubricate/config-typescript": "workspace:*",
    "@kubricate/config-vitest": "workspace:*",
    "@kubricate/mono": "workspace:*"
  },
  "dependencies": {
    "@kubricate/core": "workspace:*",
    "unconfig": "^7.3.1"
  }
}